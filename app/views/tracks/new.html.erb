<h1>Add A Track</h1>
<%= form_tag "/tracks", multipart: true do %>
  <div class='form-group row'>
    <div class="col-xs-12 col-sm-4 col-md-2">
      <%= label_tag "Title" %>
      <%= text_field_tag :name, nil, class: "form-control" %>
    </div>
  </div>
  <div class='form-group row'>
    <div class="col-xs-12 col-sm-4 col-md-2">
      <%= submit_tag 'Add Track', class: "btn btn-info" %>
    </div>
  </div>
<% end %>